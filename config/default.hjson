raft:
{
    id: "local"

    secret: ""

    data:
    {
      path: "tmp/local"
      compact:
      {
        state:
        {
          path: "../../example/passthrough_state"
        }
        watch: true
      }
    }

    peers: [
      {id: "local", url: "tcp://127.0.0.1:8047"}
    ]

    router:
    {
      bind: "tcp://*:8047"
    }

    broadcast:
    {
      url: "tcp://127.0.0.1:8048"
      bind: "tcp://*:8048"
    }

    webmonitor:
    {
      enable: true
    }
}
